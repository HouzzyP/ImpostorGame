# Configuración del Servidor El Impostor

# Puerto en el que se ejecutará el servidor
PORT=3000

# Environment (development, production)
NODE_ENV=development

# Debug mode (true/false)
DEBUG=false

# Socket.IO - CORS
SOCKET_IO_CORS_ORIGIN=*

# Configuración de Juego (valores defaults)
GAME_MAX_PLAYERS=8
GAME_MIN_PLAYERS=4
GAME_VOTING_TIME=30
GAME_DISCUSSION_TIME=30
GAME_DEFAULT_CATEGORY=random
